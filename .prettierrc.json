{
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "options": {
        "trailingComma": "es5",
        "tabWidth": 2,
        "semi": true,
        "singleQuote": true,
        "parser": "typescript",
        "arrowParens": "always",
        "bracketSpacing": true
      }
    }
  ]
}
